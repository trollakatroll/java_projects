import java.util.*;

public class HappyOrSad {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String a = sc.nextLine();
		boolean toggle = false;
		int counter1 = 0;
		int counter2 = 0;
		while(!toggle){
					if(a.indexOf(":-)") != -1){
						counter1++;
						a.replace(":-)", "");
					}else if(a.indexOf(":-(") != -1){
						counter2++;
						a.replace(":-(", "");
					}
		}
	}

}
